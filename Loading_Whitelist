    ------------------------------------Toggles------------------------------------
    
    --Main Tab--
    
    MainTab:AddLabel("Welcome To Hung Hub Premium | By hungquan99")
    
    local ServerTime = MainTab:AddLabel("Server Time")
    
    function UpdateServerTime()
        local GameTime = math.floor(workspace.DistributedGameTime+0.5)
        local Hour = math.floor(GameTime/(60^2))%24
        local Minute = math.floor(GameTime/(60^1))%60
        local Second = math.floor(GameTime/(60^0))%60
        ServerTime:Set("[Executor Time] Hours : "..Hour.." , Minutes : "..Minute.." , Seconds : "..Second)
    end
    
    spawn(function()
        while true do
            UpdateServerTime()
            wait()
        end
    end)
    
    local Client = MainTab:AddLabel("FPS Server")
    
    function UpdateClient()
        local Ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
        local Fps = workspace:GetRealPhysicsFPS()
        Client:Set("FPS : "..Fps.." , Ping : "..Ping)
    end
    
    spawn(function()
        while true do wait(.1)
            UpdateClient()
        end
    end)
    
