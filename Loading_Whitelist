    PointStats = 1
    AutoStatsTab:AddSlider({
        Name = "Point",
        Min = 1,
        Max = 100,
        Default = 1,
        Color = Color3.fromRGB(255,163,0),
        Increment = 1,
        ValueName = "Point",
