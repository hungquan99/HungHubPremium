            PointStats = value
        end    
    })
    
    spawn(function()
        while wait() do
            if game.Players.localPlayer.Data.Points.Value >= PointStats then
                if melee then
                    local args = {
                        [1] = "AddPoint",
                        [2] = "Melee",
                        [3] = PointStats
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end 
                if defense then
                    local args = {
                        [1] = "AddPoint",
                        [2] = "Defense",
                        [3] = PointStats
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end 
                if sword then
                    local args = {
                        [1] = "AddPoint",
                        [2] = "Sword",
                        [3] = PointStats
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end 
